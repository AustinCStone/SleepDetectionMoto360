    public int classify(AccelerometerData[] data){
        double x=0;
        double y=0;
        double z=0;
        for(i=0; i<data.dataLength; i++){
            x=x+data.x[i];
            y=y+data.y[i];
            z=z+data.z[i];
        }
        if(x>y && x>z)
            return 0;
        if(y>x&& y>z)
            return 1;
        return 2; 
